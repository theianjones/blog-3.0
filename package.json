{
  "name": "remix-starter-express",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "tsc -b",
    "build": "npm run css:build && remix build && tsc -b",
    "dev": "concurrently \"npm run css:watch\" \"remix run\" \"tsc -b -w --preserveWatchOutput\" \"cross-env NODE_ENV=development nodemon -w app -w build -e js,ts,tsx,md,mdx server.js\"",
    "start": "node server.js",
    "css:watch": "postcss styles --base styles --dir app/ -w",
    "css:build": "postcss styles --base styles --dir app/ --env production"
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.21",
    "@remix-run/cli": "^0.8.0",
    "@remix-run/data": "^0.8.0",
    "@remix-run/express": "^0.8.0",
    "@remix-run/react": "^0.8.0",
    "@tailwindcss/typography": "^0.3.1",
    "@tailwindcss/ui": "^0.7.2",
    "autoprefixer": "^10.0.2",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-permissions": "^1.3.3",
    "jwks-rsa": "^1.11.0",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "@theme-ui/prism": "^0.3.4",
    "@theme-ui/tailwind": "^0.3.4",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.5",
    "polished": "^4.0.4",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "12"
  }
}
